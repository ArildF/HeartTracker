<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:local="clr-namespace:WpfPrototype1Screens" x:Name="Screen_1_1_Name"
	x:Class="WpfPrototype1Screens.Screen_1_1"
	Width="640" Height="480">
	<UserControl.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="SketchStyles.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" Background="White">
		<local:Navigation HorizontalAlignment="Left" VerticalAlignment="Top" Width="146" Height="50" d:IsPrototypingComposition="True" Margin="8,120,0,0"/>
		<local:Header VerticalAlignment="Top" Height="42" d:IsPrototypingComposition="True" Margin="181,0,210,0"/>
		<ComboBox Margin="214,75,187,0" Style="{DynamicResource ComboBox-Sketch}" VerticalAlignment="Top" Height="22" MaxDropDownHeight="805">
			<ComboBoxItem/>
			<ComboBoxItem/>
			<ComboBoxItem Content="This month"/>
			<ComboBoxItem Content="Last seven days"/>
			<ComboBoxItem Content="Last 30 days"/>
		</ComboBox>
	</Grid>
</UserControl>